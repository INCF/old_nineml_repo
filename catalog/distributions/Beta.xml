<?xml version='1.0' encoding='UTF-8'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <ComponentClass name="BetaDistribution">
    <Parameter name="alpha"/>
    <Parameter name="beta"/>
    <PropertySendPort name="weight" dimension="dimensionless"/>
    <Distribution>
      <RandomVariable name="weight" dimension="dimensionless">
        <un:beta xmlns:un="http://www.uncertml.org/2.0">
          <un:alpha>alpha</un:alpha>
          <un:beta>beta</un:beta>
        </un:beta>
      </RandomVariable>
    </Distribution>
  </ComponentClass>
  <Dimension name="dimensionless"/>
</NineML>
