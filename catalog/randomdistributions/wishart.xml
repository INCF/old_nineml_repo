<?xml version='1.0' encoding='UTF-8'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <ComponentClass name="WishartDistribution">
    <Parameter name="degreesOfFreedom"/>
    <Parameter name="scaleMatrix"/>
    <PropertySendPort name="weight" dimension="dimensionless"/>
    <Distribution>
      <RandomVariable name="weight" dimension="dimensionless">
        <un:wishart xmlns:un="http://www.uncertml.org/2.0">
          <un:degreesOfFreedom>degreesOfFreedom</un:degreesOfFreedom>
          <un:scaleMatrix>scaleMatrix</un:scaleMatrix>
        </un:wishart>
      </RandomVariable>
    </Distribution>
  </ComponentClass>
</NineML>
