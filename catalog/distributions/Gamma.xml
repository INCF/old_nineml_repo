<?xml version='1.0' encoding='UTF-8'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <ComponentClass name="GammaDistribution">
    <Parameter name="shape"/>
    <Parameter name="scale"/>
    <PropertySendPort name="weight" dimension="dimensionless"/>
    <Distribution>
      <RandomVariable>
        <un:gamma xmlns:un="http://www.uncertml.org/2.0">
          <un:shape>shape</un:shape>
          <un:shape>scale</un:shape>
        </un:gamma>
      </RandomVariable>
     </Distribution>
  </ComponentClass>
  <Dimension name="dimensionless"/>
</NineML>
