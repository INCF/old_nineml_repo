<?xml version='1.0' encoding='UTF-8'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <ComponentClass name="ExponentialDistribution">
    <Parameter name="rate"/>
    <PropertySendPort name="weight" dimension="dimensionless"/>
    <Distribution>
      <RandomVariable name="weight" dimension="dimensionless">
        <un:exponential xmlns:un="http://www.uncertml.org/2.0">
          <un:rate>rate</un:rate>
        </un:exponential>
      </RandomVariable>
    </Distribution>
  </ComponentClass>
</NineML>
