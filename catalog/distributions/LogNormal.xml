<?xml version='1.0' encoding='UTF-8'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <ComponentClass name="LogNormalDistribution">
    <Parameter name="logScale"/>
    <Parameter name="shape"/>
    <PropertySendPort name="weight" dimension="dimensionless"/>
    <Distribution>
      <RandomVariable>
        <un:logNormalDistribution xmlns:un="http://www.uncertml.org/2.0">
          <un:logScale>logScale</un:logScale>
          <un:shape>shape</un:shape>
        </un:logNormalDistribution>
      </RandomVariable>
    </Distribution>
  </ComponentClass>
  <Dimension name="dimensionless"/>
</NineML>
