<?xml version='1.0' encoding='UTF-8'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <ComponentClass name="CauchyDistribution">
    <Parameter name="location"/>
    <Parameter name="scale"/>
    <PropertySendPort name="weight" dimension="dimensionless"/>
    <Distribution>
      <RandomVariable name="weight" dimension="dimensionless">
        <un:cauchy xmlns:un="http://www.uncertml.org/2.0">
          <un:location>location</un:location>
          <un:scale>scale</un:scale>
        </un:cauchy>
      </RandomVariable>
    </Distribution>
  </ComponentClass>
  <Dimension name="dimensionless"/>
</NineML>
