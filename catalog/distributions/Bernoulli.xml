<?xml version='1.0' encoding='UTF-8'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <ComponentClass name="BernoulliDistribution">
    <Parameter name="probabilities"/>
    <PropertySendPort name="weight" dimension="dimensionless"/>
    <Distribution>
      <RandomVariable name="weight" dimension="dimensionless">
        <un:bernoulli xmlns:un="http://www.uncertml.org/2.0">
          <un:probabilities>probabilities</un:probabilities>
        </un:bernoulli>
      </RandomVariable>
    </Distribution>
  </ComponentClass>
  <Dimension name="dimensionless"/>
</NineML>
