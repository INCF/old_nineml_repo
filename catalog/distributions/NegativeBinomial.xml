<?xml version='1.0' encoding='UTF-8'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <ComponentClass name="NegativeBinomialDistribution">
    <Parameter name="numberOfFailures"/>
    <Parameter name="probability"/>
    <PropertySendPort name="weight" dimension="dimensionless"/>
    <Distribution>
      <RandomVariable name="weight" dimension="dimensionless">
        <un:negativeBinomialDistribution xmlns:un="http://www.uncertml.org/2.0">
          <un:numberOfFailures>numberOfFailures</un:numberOfFailures>
          <un:probability>probability</un:probability>
        </un:negativeBinomialDistribution>
      </RandomVariable>
    </Distribution>
  </ComponentClass>
  <Dimension name="dimensionless"/>
</NineML>
