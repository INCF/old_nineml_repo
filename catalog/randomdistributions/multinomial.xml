<?xml version='1.0' encoding='UTF-8'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <ComponentClass name="MultinomialDistribution">
    <Parameter name="numberOfTrials"/>
    <Parameter name="probabilities"/>
    <PropertySendPort name="weight" dimension="dimensionless"/>
    <Distribution>
      <RandomVariable name="weight" dimension="dimensionless">
        <un:multinomial xmlns:un="http://www.uncertml.org/2.0">
          <un:numberOfTrials>numberOfTrials</un:numberOfTrials>
          <un:probabilities>probabilities</un:probabilities>
        </un:multinomial>
      </RandomVariable>
    </Distribution>
  </ComponentClass>
</NineML>
