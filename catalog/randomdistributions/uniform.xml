<?xml version='1.0' encoding='UTF-8'?>
<NineML xmlns="http://nineml.net/9ML/1.0">
  <ComponentClass name="UniformDistribution">
    <Parameter name="minimum"/>
    <Parameter name="maximum"/>
    <PropertySendPort name="weight" dimension="dimensionless"/>
    <Distribution>
      <RandomVariable name="weight" dimension="dimensionless">
        <un:uniform xmlns:un="http://www.uncertml.org/2.0">
          <un:minimum>minimum</un:minimum>
          <un:maximum>maximum</un:maximum>
        </un:uniform>
      </RandomVariable>
    </Distribution>
  </ComponentClass>
</NineML>
